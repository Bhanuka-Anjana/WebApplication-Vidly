@model List<Movie>
  
@{
	ViewBag.Title = "Movies";
	Layout = "~/Views/Shared/_Layout.cshtml";
}

<table class="table">
	<thead>
		<tr>
			<th scope="col">Id</th>
			<th scope="col">Title</th>
			<th scope="col">Description</th>
		</tr>
	</thead>
	<tbody>
		@foreach (var movie in Model)
		{
			<tr>
				<th scope="row">@movie.Id</th>
				<td>@movie.Title</td>
				<td>@movie.Description</td>
			</tr>
		}
	</tbody>
</table>
